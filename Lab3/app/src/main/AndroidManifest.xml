        <!--  Пятая страница -->
        <activity android:name=".FiveStep">
        </activity>
        <!--  Шестая страница -->
        <activity android:name=".SixStep">
        </activity>
        <!--  Седьмая страница -->
        <activity android:name=".SevenStep">
        </activity>
        <!--  Восьмая страница -->
